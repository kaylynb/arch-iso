{
	"hostname": "vm",
	"localtime": "America/Los_Angeles",
	"parted": {
		"disk": "/dev/sda",
		"commands": [
			"mklabel gpt",
			"mkpart ESP fat32 1MiB 513MiB",
			"set 1 boot on",
			"mkpart primary ext4 513MiB 100%"
		]
	},
	"fs": {
		"boot": {
			"format": true,
			"disk": "/dev/sda1"
		},
		"system": {
			"disk": "/dev/sda2"
		}
	},
	"packages": {
		"iso": {
			"dependencies": [],
			"packages": [
				"terminus-font"
			]
		},
		"system": {
			"aur": [],
			"packages": [
				"terminus-font"
			]
		}
	},
	"consolefont": "ter-v28n"
}
