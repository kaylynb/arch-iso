{
	"hostname": "rho",
	"localtime": "America/Los_Angeles",
	"network_wired": true,
	"user_snapshots": true,
	"parted": {
		"disk": "/dev/sdb",
		"commands": [
			"mklabel gpt",
			"mkpart ESP fat32 1MiB 513MiB",
			"set 1 boot on",
			"mkpart Arch ext4 513MiB 100%"
		]
	},
	"fs": {
		"boot": {
			"format": true,
			"disk": "/dev/sdb1"
		},
		"system": {
			"encrypted": true,
			"disk": "/dev/sdb2"
		}
	},
	"packages": {
		"iso": {
			"dependencies": [],
			"packages": []
		},
		"system": {
			"aur": [],
			"packages": []
		}
	}
}
