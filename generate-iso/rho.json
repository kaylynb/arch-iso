{
	"hostname": "rho",
	"localtime": "America/Los_Angeles",
	"parted": {
		"disk": "/dev/sdd",
		"commands": [
			"mklabel gpt",
			"mkpart ESP fat32 1MiB 513MiB",
			"set 1 boot on",
			"mkpart primary ext4 513MiB 100%"
		]
	},
	"fs": {
		"boot": {
			"format": true,
			"disk": "/dev/sdd1"
		},
		"system": {
			"disk": "/dev/sdd2"
		}
	},
	"packages": {
		"iso": {
			"dependencies": [],
			"packages": []
		},
		"system": {
			"aur": [
				"https://aur.archlinux.org/cgit/aur.git/snapshot/aura-bin.tar.gz"
			],
			"packages": []
		}
	},
	"mkinitcpio": {
		"hooks": [
			"base",
			"udev",
			"autodetect",
			"modconf",
			"block",
			"keymap",
			"keyboard",
			"encrypt",
			"lvm2",
			"filesystems",
			"fsck"
		]
	}
}
